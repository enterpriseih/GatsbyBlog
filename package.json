{
    "name": "towavephone.com",
    "description": "My personal home page",
    "private": true,
    "lint-staged": {
        "*.{js,jsx}": [
            "./node_modules/.bin/eslint --fix",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
          "pre-commit": "yarn lint:markdown && lint-staged"
        }
    },
    "scripts": {
        "build": "gatsby build --prefix-paths",
        "deploy": "gh-pages -d public -r git@github.com:towavephone/towavephone.github.io.git -b master",
        "dev": "gatsby develop -p 80",
        "serve": "gatsby serve",
        "lint": "yarn lint:markdown && yarn lint:js",
        "lint:js": "./node_modules/.bin/eslint --fix src/**/*.js gatsby-config.js gatsby-browser.js gatsby-node.js",
        "test": "echo \"Should probably write some tests!\"",
        "lint:markdown": "markdownlint --config ./.markdownlint.json ./blog/**/*.md",
        "precommit": "yarn lint:markdown && lint-staged"
    },
    "devDependencies": {
        "eslint": "^4.19.1",
        "eslint-config-prettier": "^2.9.0",
        "eslint-plugin-prettier": "^2.6.0",
        "eslint-plugin-react": "^7.7.0",
        "gatsby-cli": "^1.1.52",
        "gh-pages": "^1.1.0",
        "husky": "^1.0.0-rc.2",
        "lint-staged": "^7.1.0",
        "markdownlint": "^0.8.1",
        "markdownlint-cli": "^0.8.1",
        "prettier": "^1.12.1"
    },
    "dependencies": {
        "bootstrap-sass": "^3.3.7",
        "dotenv": "^5.0.1",
        "fontfaceobserver": "^2.0.13",
        "gatsby": "^1.9.260",
        "gatsby-image": "^1.0.51",
        "gatsby-link": "^1.6.44",
        "gatsby-plugin-algolia": "^0.0.3",
        "gatsby-plugin-canonical-urls": "^1.0.18",
        "gatsby-plugin-catch-links": "^1.0.22",
        "gatsby-plugin-feed": "^1.3.22",
        "gatsby-plugin-google-analytics": "^1.0.31",
        "gatsby-plugin-manifest": "^1.0.13",
        "gatsby-plugin-nprogress": "^1.0.14",
        "gatsby-plugin-offline": "^1.0.16",
        "gatsby-plugin-react-helmet": "^2.0.11",
        "gatsby-plugin-sass": "^1.0.26",
        "gatsby-plugin-sharp": "^1.6.44",
        "gatsby-plugin-sitemap": "^1.2.23",
        "gatsby-remark-autolink-headers": "^1.4.18",
        "gatsby-remark-design-system": "^1.0.9",
        "gatsby-remark-embed-snippet": "^1.0.22",
        "gatsby-remark-emoji": "0.0.2",
        "gatsby-remark-external-links": "0.0.4",
        "gatsby-remark-images": "^1.5.63",
        "gatsby-remark-katex": "^1.0.14",
        "gatsby-remark-prismjs": "^2.0.2",
        "gatsby-remark-responsive-iframe": "^1.4.18",
        "gatsby-remark-smartypants": "^1.4.12",
        "gatsby-source-filesystem": "^1.5.35",
        "gatsby-transformer-remark": "^1.7.41",
        "gatsby-transformer-sharp": "^1.6.24",
        "gitalk": "^1.2.2",
        "instantsearch.css": "^7.1.0",
        "prop-types": "^15.6.1",
        "react-helmet": "^5.2.0",
        "react-instantsearch": "^5.0.3",
        "react-share": "^2.1.1"
    }
}
